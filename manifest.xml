<package>
  <description brief="Whole Body control library">
  Implementations of algorithms for whole body robot control, including redundancy resolution based on hierarchical null space decomposition. Should cover velocity, acceleration and force based control.
  </description>
  <author>Dennis Mronga/dennis.mronga@dfki.de</author>
  <maintainer>Dennis Mronga/dennis.mronga@dfki.de</maintainer>
  <license></license>
  <url>https://git.hb.dfki.de/dfki-control/wbc</url>
  <logo>https://git.hb.dfki.de/dfki-control/wbc/blob/master/doc/images/wbc_principle.png</logo>
  <tags>stable</tags>
  <keywords>
     <keyword>Control</keyword>
     <keyword>Whole-Body Control</keyword>
     <keyword>Optimization</keyword>
     <keyword>Redundancy Resolution</keyword>
   </keywords>

  <stage>0</stage>
  <use>single project</use>
  <activity>active</activity>

  <classifiers>
    <classifier>c++</classifier>
    <classifier>rock</classifier>
    <classifier>ubuntu</classifier>
  </classifiers>

  <depend package="base/cmake" />
  <depend package="control/kdl_conversions" />
  <depend package="eigen3" />
  <depend package="control/kdl_parser" />
  <depend package="control/wbc_common" />
</package>
