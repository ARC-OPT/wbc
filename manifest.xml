  <classifiers>
    <classifier>c++</classifier>
    <classifier>rock</classifier>
    <classifier>ubuntu</classifier>
  </classifiers>

  <depend package="base/cmake" />
  <depend package="control/kdl_conversions" />
  <depend package="eigen3" />
