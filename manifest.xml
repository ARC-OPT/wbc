     <keyword>Control</keyword>
     <keyword>Whole-Body Control</keyword>
     <keyword>Optimizationl</keyword>
     <keyword>Redundancy Resolution</keyword>
   </keywords>

  <stage>1</stage>
  <use>single project</use>
